





Function Main()
   LOCAL oHttp, cHtml

   oHttp:= TIpClientHttp():new( "http://parallelum.com.br/fipe/api/v1/carros/marcas" )

   IF oHttp:open()
      cHtml   := oHttp:readAll()

      aMarcas := {}
      hMarcas := { => }

      HB_JsonEncode( cHtml, @aMarcas )
      hMarcas["ITEMS"] := aMarcas

      QOut( hMarcas["ITEMS"][1] )

      QOut( aMarcas )



      oHttp:close()
      QOut( cHtml )
   ELSE
      QOut( "erro de conexao:", oHttp:lastErrorMessage() )
   ENDIF

Return
