

Function Main()
   LOCAL oHttp, cHtml, hQuery

   oHttp:= TIpClientHttp():new( "http://parallelum.com.br/fipe/api/v1/carros/marcas" )


   hQUery := Hash()
   hSetCaseMatch( hQuery, .F. )

   hQuery["q"]    := "xHarbour"
